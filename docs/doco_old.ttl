  rdfs:subClassOf [
      a owl:Restriction ;
      owl:onProperty pattern:contains ;
      owl:someValuesFrom [
          a owl:Class ;
          owl:unionOf (
              <http://purl.org/spar/doco/ttlOrigParagraph>
