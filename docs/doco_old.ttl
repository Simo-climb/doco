          <http://purl.org/spar/doco/ttlOrigSection>
          [
            a owl:Restriction ;
            owl:onProperty terms:isPartOf ;
            owl:someValuesFrom [
                a owl:Class ;
                owl:unionOf (
                    <http://purl.org/spar/doco/ttlOrigBackMatter>
                    <http://purl.org/spar/doco/ttlOrigFrontMatter>
